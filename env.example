# PartKeeper Environment Variables
# Copy this file to .env.local and update the values

# Access code for authentication
# Users will need this code to access the app
NEXT_PUBLIC_ACCESS_CODE=partkeeper2024

# Songs backend: "file" for local JSON (default), later: "postgres", "mongodb", etc.
NEXT_PUBLIC_SONGS_BACKEND=file
# If backend=file, path to songs.json (defaults to public/data/songs.json)
SONGS_FILE_PATH=

# Optional: Set to 'production' for production builds
# NODE_ENV=development
